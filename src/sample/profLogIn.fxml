<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label layoutX="171.0" layoutY="38.0" text="Professor Log In">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="txtProfEmail" layoutX="200.0" layoutY="139.0" prefHeight="30.0" prefWidth="200.0" promptText="Email">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="txtProfPassword" layoutX="200.0" layoutY="231.0" prefHeight="30.0" prefWidth="200.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="btnProfLog" layoutX="200.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleBtnProfLog" prefHeight="39.0" prefWidth="60.0" text="Log In" />
      <Button fx:id="btnProfLogGoBack" layoutX="348.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleBtnProfLogGoBack" prefHeight="39.0" prefWidth="60.0" text="Go Back" />
   </children>
</AnchorPane>
